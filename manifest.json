{
    "manifest_version": 3,
    "name": "BirdX",
    "version": "0.0.1",
    "description": "X out the X",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
      },
      "icons": {
        "16": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://twitter.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["activeTab", "storage"]
  }
  